{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'warehouse-grid' %}
{% set pageTitle = 'rockordier.warehouse.entity_plural_label'|trans %}

{% block navButtons %}

    {% if resource_granted('rockordier_warehouse_create') %}
        <div class="btn-group">
            {{ UI.addButton({
                'path' : path('rockordier_warehouse_create'),
                'entity_label': 'rockordier.warehouse.entity_label'|trans
            }) }}
        </div>
    {% endif %}
{% endblock %}